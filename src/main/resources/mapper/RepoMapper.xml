<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="team.weilai.studythrough.mapper.RepoMapper">

    <resultMap id="BaseResultMap" type="team.weilai.studythrough.pojo.exam.Repo">
            <id property="repoId" column="repo_id" jdbcType="BIGINT"/>
            <result property="repoTitle" column="repo_title" jdbcType="VARCHAR"/>
            <result property="subject" column="subject" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        repo_id,repo_title,subject,
        create_time
    </sql>
</mapper>
